# Set Flask environment.
FLASK_ENV=production

# Set gunicorn listen port.
PORT=8000

# Number of gunicorn workers. When run in a "production" environment.
WEB_CONCURRENCY=4

# Gremlin endpoint.
GREMLIN_ENDPOINT='wss://neptune-endpoint:8182/gremlin'

# Use IAM credentials for Gremlin authentication.
GREMLIN_AUTH_MODE='neptune_iam'
